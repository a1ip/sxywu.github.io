{
  "name": "sxywu.com",
  "main": "scripts/main.js",
  "dependencies": {
    "d3": "^3.5.17",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify scripts/main.js -o scripts/bundle.js -v -d",
    "build": "NODE_ENV=production browserify . | uglifyjs -cm > scripts/bundle.min.js"
  },
  "browserify": {
    "transform": [
      ["reactify", {"es6": true}],
      "envify"
    ]
  }
}